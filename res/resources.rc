//
// Menu resources
//
LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
IDR_MENU MENU
{
    POPUP "&File"
    {
        POPUP "&New"
        {
            MENUITEM "Mod &Hub...", IDM_FILE_NEW_CTX
        }
        MENUITEM SEPARATOR
        MENUITEM "&Open\tCtrl+O", IDM_FILE_OPEN
        POPUP "&Recent", GRAYED
        {
            MENUITEM SEPARATOR
            MENUITEM "&Clear history", IDM_FILE_CLEAR_HIST
        }
        MENUITEM SEPARATOR
        MENUITEM "&Close\tCtrl+W", IDM_FILE_CLOSE, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&Quit\tCtrl+Q", IDM_FILE_QUIT
    }
    POPUP "&Edit"
    {
        POPUP "Mod &Hub", GRAYED
        {
          MENUITEM "&Add Mod Channel...", IDM_EDIT_ADD_CHN
          MENUITEM SEPARATOR
          MENUITEM "&Properties...", IDM_EDIT_CTX_PROP
        }
        MENUITEM SEPARATOR
        POPUP "Mod &Channel", GRAYED
        {
          MENUITEM "&Properties...", IDM_EDIT_CHN_PROP
        }
        MENUITEM SEPARATOR
        POPUP "Mod &Package", GRAYED
        {
            MENUITEM "&Install\tEnter", IDM_EDIT_PKG_INST
            MENUITEM "&Uninstall\tBackspace", IDM_EDIT_PKG_UINS
            MENUITEM "Uninstall &tree\tShift+Backspace", IDM_EDIT_PKG_CLNS
            MENUITEM SEPARATOR
            MENUITEM "Di&scard backup data\tCtrl+Shift+Backspace", IDM_EDIT_PKG_DISC
            MENUITEM SEPARATOR
            MENUITEM "&Open in Explorer...\tCtrl+E", IDM_EDIT_PKG_OPEN
            MENUITEM "Move to &recycle bin\tDelete", IDM_EDIT_PKG_TRSH
            MENUITEM SEPARATOR
            MENUITEM "Load in Package &Editor...\tCtrl+S", IDM_EDIT_PKG_EDIT
            MENUITEM SEPARATOR
            MENUITEM "View &details...\tCtrl+I", IDM_EDIT_PKG_INFO
        }
        POPUP "&Remote", GRAYED
        {
            MENUITEM "&Download\tEnter", IDM_EDIT_RMT_DNLD
            MENUITEM "Download &without supersede\tShift+Enter", IDM_EDIT_RMT_DNWS
            MENUITEM SEPARATOR
            MENUITEM "Fix de&pendencies\tCtrl+Enter", IDM_EDIT_RMT_FIXD
            MENUITEM SEPARATOR
            MENUITEM "View &details...\tCtrl+I", IDM_EDIT_RMT_INFO
        }
        MENUITEM SEPARATOR
        MENUITEM "&Manager options...", IDM_EDIT_OPTIONS
    }
    POPUP "&Tools"
    {
        MENUITEM "&Package editor...", IDM_TOOLS_EDI_PKG
        MENUITEM "&Repository editor...", IDM_TOOLS_EDI_REP
    }
    POPUP "&Help"
    {
        MENUITEM "Debug &log...\tCtrl+D", IDM_HELP_LOG
        MENUITEM "About...", IDM_HELP_ABOUT
    }
}

LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
IDR_MENU_BAT  MENU
{
  POPUP "&Script"
  {
    MENUITEM "&New...", IDM_BATCH_NEW
    MENUITEM SEPARATOR
    MENUITEM "&Launch", IDM_BATCH_EXE
    MENUITEM "&Properties...", IDM_BATCH_EDI
    MENUITEM SEPARATOR
    MENUITEM "&Delete", IDM_BATCH_DEL
  }
}

